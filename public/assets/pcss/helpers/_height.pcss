}
.ts-min-h__50vh {
  min-height: 50vh;
}
.ts-min-h__60vh {
  min-height: 60vh;
}
.ts-min-h__70vh {
  min-height: 70vh;
}
.ts-min-h__80vh {
  min-height: 80vh;
}
.ts-min-h__90vh {
