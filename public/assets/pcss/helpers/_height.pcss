/* SM*/
@media (min-width: 576px) {
  .ts-sm-h__auto {
    height: auto;
  }
}

/*MD*/
@media (min-width: 768px) {
  .ts-md-h__auto {
    height: auto;
  }
}

/*LG*/
@media (min-width: 992px) {
  .ts-lg-h__auto {
    height: auto;
  }
}

/*XL*/
@media (min-width: 1200px) {
  .ts-xl-h__auto {
    height: auto;
  }
}


