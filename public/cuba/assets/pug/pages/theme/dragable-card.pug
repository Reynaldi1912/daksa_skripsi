- var theme_customizer      = true;
- var jquery_ui             = true;
- var dragable              = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var scrollbar             = true;
- var height_equal          = true;
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid
            .page-title
              .row
                .col-6
                  h4
                    | Draggable Card
                .col-6
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Bonus Ui
                    li.breadcrumb-item.active Draggable Card
          // Container-fluid starts
          .container-fluid
            #draggableMultiple.row.ui-sortable
              .col-xxl-4.col-md-6
                .card.height-equal
                  .card-header
                    h4 Basic Card
                    p.mt-1.f-m-light
                        | You can draggable cards anywhere.
                  .card-body
                    ul.list-group
                      li.list-group-item 
                        i.icofont.icofont-arrow-right
                        | Logo Design
                      li.list-group-item 
                        i.icofont.icofont-arrow-right
                        | Web Designer 
                      li.list-group-item 
                        i.icofont.icofont-arrow-right
                        | E-Commerce
                      li.list-group-item 
                        i.icofont.icofont-arrow-right
                        | UX Designer
              .col-xxl-4.col-md-6
                .card.height-equal
                  .card-header
                    h4 Default Card
                    p.mt-1.f-m-light
                        | You can draggable cards anywhere.
                  .card-body
                    ul.list-group
                      li.list-group-item.active.bg-warning-light(aria-current='true')
                        i.icofont.icofont-arrow-right 
                        | UI Kits
                      li.list-group-item
                        i.icofont.icofont-arrow-right 
                        | Wow Animations
                      li.list-group-item 
                        i.icofont.icofont-arrow-right 
                        |Apex Charts
                      li.list-group-item
                        i.icofont.icofont-arrow-right 
                        | Starter Kits
              .col-xxl-4.col-xl-6.col-12
                .card.b-r-0.height-equal
                  .card-header
                    h4 Flat Card
                    p.f-m-light.mt-1
                      | You can draggable cards anywhere.
                  .card-body
                    ul.list-group.list-group-flush
                      li.list-group-item
                        i.icofont.icofont-arrow-right 
                        | PRODUCT
                      li.list-group-item
                        i.icofont.icofont-arrow-right 
                        | PRODUCT DETAILS 
                      li.list-group-item
                        i.icofont.icofont-arrow-right 
                        | CART 
                      li.list-group-item.pb-0
                        i.icofont.icofont-arrow-right 
                        | CHECKOUT
              .col-sm-12.col-xl-6
                .card.shadow-none.border.height-equal
                  .card-header
                    h4 Without Shadow Card
                    p.f-m-light.mt-1
                      | You can draggable cards anywhere.
                  .card-body
                    .alert.txt-primary.border-primary.alert-dismissible.fade.show(role='alert')
                      i(data-feather='clock')
                      p
                        |One of 
                        strong YouTube's
                        | most crucial ranking factors is Watch Time.
                      button.btn-close(type='button', aria-label='Close')
                    .alert.txt-success.border-success.outline-2x.alert-dismissible.fade.show.alert-icons(role='alert')
                      i(data-feather='thumbs-up')
                      p
                        b  Well done! 
                        | You successfully read this important message.
                      button.btn-close(type='button', aria-label='Close')
              .col-sm-12.col-xxl-6
                .card.height-equal
                  .card-header
                    h4
                      i.icon-move.me-2
                      |  Icon In Heading
                    p.f-m-light.mt-1
                      | You can draggable cards anywhere.
                  .card-body
                    .alert.border-warning.alert-dismissible.fade.show.p-0(role='alert')
                      .alert-arrow.bg-warning
                        i.icon-timer
                      p
                        | Your time Over after 5 minute                          
                    .alert.border-danger.alert-dismissible.fade.show.p-0.mb-0(role='alert')
                      .alert-arrow.bg-danger
                        i.icon-heart
                      p
                        | Oh snap! Change a few things up notification check
          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
